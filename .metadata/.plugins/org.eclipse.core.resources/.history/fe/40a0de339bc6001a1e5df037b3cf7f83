package perushaan1;
import java.util.*;
public class Student { 
	  
	  
	    public static void main(String args[]) 
	    { 
	        ArrayList<String> al 
	            = new ArrayList<>(); 
	  
	        al.add("Geeks"); 
	        al.add("Geeks"); 
	        al.add(2, "For"); 
	  
	        // Using the Get method and the 
	        // for loop 
	        for (int i = 0; i < al.size(); i++) { 
	  
	            System.out.print(al.get(i) + " "); 
	        } 
	  
	        System.out.println(); 
	  
	        // Using the for each loop 
	        for (String str : al) 
	            System.out.print(str + " "); 
	    } 
	} 

